{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "chatykick",
  "version": "0.1.0",
  "identifier": "dev.titoinemb.chatykick",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "chatykick",
        "width": 420,
        "height": 820,
        "resizable": true,
        "maxWidth": 500,
        "minWidth": 400
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IERDMTJEOTIwMzFEN0Y3NzcKUldSMzk5Y3hJTmtTM0puVnNmNFZTeUlkV3Vzb0d1WU9uMEo1aFRiQjBla0JtczlBSVgwajE4V0kK",
      "endpoints": [
        "https://github.com/titoinemb/ChatyKick/releases/latest/download/latest.json"
      ],
      "windows": {
        "installMode": "passive"
      }
    }
  }
}